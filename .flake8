[flake8]
max-line-length = 88
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .pytest_cache,
    venv,
    env,
    .venv,
    migrations/,
    build/,
    dist/
ignore = 
    E203,
    W503,
    E501
per-file-ignores =
    __init__.py:F401
select = E,W,F,C
statistics = True
count = True